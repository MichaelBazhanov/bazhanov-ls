.thumbs {
	position: relative;
	display: flex;
}
.thumbs__item-active {
	transition: all .3s;
	box-shadow: 0 0 0 3px $links-color-active;
	z-index: 1;
}
.thumbs__item {
	width: 178px;
	height: 100px;
	transition: all .3s
}
.thumbs__link {
	width: 100%;
	height: 100%;
}
.thumbs__pic {
	width: 100%;
	height: 100%;
	object-fit: cover;
}